{
  "prompts": {
    "frame": {
      "type": 1,
      "list": ["none", "vue", "react"],
      "default": 0
    },
    "name": {
      "type": 0
    },
    "refresh": {
      "type": 1,
      "list": {"poll": "Use polling", "socketio": "Use socket.io"},
      "default": 0
    }
  },
  "filters": {
    "include": {
      "dist": "frame:react"
    },
    "exclude": {
      "src/app.js": "frame:none"
    }
  },
  "message": "Welcome to use {frame}!"
}
