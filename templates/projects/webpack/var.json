{
  "prompts": {
    "name": {
      "type": 0
    },
    "frame": {
      "type": 1,
      "list": ["none", "vue", "react"],
      "default": 0
    }
  },
  "filters": {
    "include": {
      "src/app.js": "frame:react",
      "src/app.vue": "frame:vue"
    }
  },
  "message": "Welcome to use webpack and {frame}!"
}
